    <header id="header">
        <div class="content">

            <nav id="nav">
                <div class="col-lg-6 col-md-4" id="primeracol">
                <ul>
                    <li><a href="#" id="fundaciones">Fundaciones</a></li>
                    <li class="active"><a href="http://bootsnipp.com/snippets/featured/nav-account-manager" target="_blank" id="donantes">Donantes</a></li>
                    <li><a href="#" id="nosotros">Nosotros</a></li>
                </div>

                <div class="col-lg-offset-2 col-lg-4 col-md-offset-4 col-md-4" id="segundacol">
                    <% if user_signed_in? %>
                    <ul class="nav navbar-nav navbar-right">
                        <li class="dropdown">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="glyphicon glyphicon-user"></span> 
                                <strong><%= @user.username%></strong>
                                <span class="glyphicon glyphicon-chevron-down"></span>
                            </a>
                            <ul class="dropdown-menu">
                                <li>
                                    <div class="navbar-login">
                                        <div class="row">
                                            <div class="col-lg-4">
                                                <p class="text-center">
                                                    <span class="glyphicon glyphicon-user icon-size"></span>
                                                </p>
                                            </div>
                                            <div class="col-lg-8">
                                                <p class="text-left"><strong><%= @user.username%></strong></p>
                                                <p class="text-left small"><%= @user.email%></p>
                                                <p class="text-left">
                                                    <%= link_to 'Actualizar datos', complete_profile_path ,:method => :patch , :class => "btn btn-primary btn-block"%>
                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                                <li class="divider"></li>
                                <li>
                                    <div class="navbar-login navbar-login-session">
                                        <div class="row">
                                            <div class="col-lg-12">
                                                <p>
                                                    
                                                    <%= link_to 'Cerrar Sesión', destroy_user_session_path, :method => :delete , :class => "btn btn-danger btn-block" %>

                                                </p>
                                            </div>
                                        </div>
                                    </div>
                                </li>
                            </ul>
                        </li>
                    </ul>

                    <% else %>
                     <ul class="nav navbar-nav navbar-right">
                        

                        <li id="registrate">
                            <%= link_to "Regístrate", new_user_registration_path %>      
                        </li>
                        <li id="ingresa">
                            <%= link_to "Ingresa", new_user_session_path %>
                        </li>

                     </ul>
                    <% end %>

                </div>
            </div>
            </nav>

        </div>
    </header>




